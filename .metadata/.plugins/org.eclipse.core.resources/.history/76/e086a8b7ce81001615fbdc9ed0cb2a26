package char1;

public class Char {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		  char ch1 = 'a';
          char ch2 = 65; /* ASCII code of 'A'*/
         
          System.out.println("Value of char variable ch1 is :" + ch1);   
          System.out.println("Value of char variable ch2 is :" + ch2);   
	}

}
